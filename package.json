{
  "name": "coolshapes-vue",
  "description": "A vue component library for coolshapes with little grainy gradients.",
  "version": "0.1.0",
  "type": "module",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "scripts": {
    "dev": "vite --port 5353",
    "build:docs": "vite build --mode docs",
    "build:lib": "vite build --mode lib && vue-tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/coolshapes-vue.git"
  },
  "homepage": "https://github.com/xiaoluoboding/coolshapes-vue",
  "files": [
    "lib"
  ],
  "keywords": [
    "coolshapes",
    "shapes",
    "abstract",
    "svg",
    "vector",
    "avatar",
    "illustration",
    "graphics",
    "placeholder"
  ],
  "main": "./lib/coolshapes-vue.umd.cjs",
  "module": "./lib/coolshapes-vue.js",
  "exports": {
    ".": {
      "import": "./lib/coolshapes-vue.js",
      "require": "./lib/coolshapes-vue.umd.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@iconify/json": "^2.2.207",
    "@types/node": "^18.19.31",
    "@unocss/reset": "^0.55.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vueuse/core": "^10.9.0",
    "clean-css": "^5.3.3",
    "highlight.js": "^11.9.0",
    "sass": "^1.76.0",
    "typescript": "^4.9.5",
    "unocss": "^0.55.7",
    "unplugin-icons": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.3",
    "vue": "^3.4.26",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@unhead/vue": "^1.9.10",
    "copy-to-clipboard": "^3.3.3",
    "unhead": "^1.9.10"
  }
}